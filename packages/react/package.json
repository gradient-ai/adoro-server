    "ahooks": "^2.10.9",
    "framer-motion": "^4.1.17",
    "next": "^11.1.0",
    "next-pwa": "^5.3.1",
    "react": "^17.0.2",
    "react-confetti": "^6.0.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-share": "^4.4.0",
    "react-use": "^17.2.4",
    "react-webcam": "^6.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.1.0",
    "@types/react": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "commitlint": "^13.1.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "typescript": "^4.4.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
